<core:FragmentDefinition  xmlns:core="sap.ui.core" xmlns:m="sap.m" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <m:IconTabBar class="sapUiResponsiveContentPadding" id="iconTabBar">
        <m:items>
            <m:IconTabFilter icon="sap-icon://database" text="Basic" id="iconTabBarFilter1" tooltip="Basic">
                <m:Table busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin" id="lineItemsList3" items="{Product_Class/Basic}"
                         noDataText="{i18n>detailLineItemTableNoDataText}" updateFinished="onListUpdateFinishedDocuments" width="60%">
                    <m:columns>
                        <m:Column width="30%">
                            <m:Text text="Characteristic"/>
                        </m:Column>
                        <m:Column>
                            <m:Text text="Value"/>
                        </m:Column>
                    </m:columns>
                    <m:items>
                        <m:ColumnListItem>
                            <m:cells>
                                <m:ObjectIdentifier text="{char}" id="char" />
                                <m:Input editable="{classView>/editable}" value="{value}" id="val" app:multi="{multi}" app:value_model="{type}" showValueHelp="true" valueHelpRequest="getF4"/>
                            </m:cells>
                        </m:ColumnListItem>
                    </m:items>
                </m:Table>
            </m:IconTabFilter>

            <m:IconTabFilter icon="sap-icon://bus-public-transport" text="Publication" id="iconTabBarFilter2" tooltip="Publication">

                <m:Table busyIndicatorDelay="{detailView>/lineItemTableDelay}" class="sapUiResponsiveMargin" id="lineItemsList4" items="{Product_Class/Publication}"
                         noDataText="{i18n>detailLineItemTableNoDataText}" updateFinished="onListUpdateFinishedDocuments" width="60%">
                    <m:columns>
                        <m:Column width="30%">
                            <m:Text text="Characteristic"/>
                        </m:Column>
                        <m:Column>
                            <m:Text text="Value"/>
                        </m:Column>
                    </m:columns>
                    <m:items>
                        <m:ColumnListItem>
                            <m:cells>
                                <m:ObjectIdentifier text="{char}" />
                                <m:Input editable="{classView>/editable}" value="{value}" id="publ1" app:multi="{multi}" app:value_model="{type}" showValueHelp="true" valueHelpRequest="getF4"/>
                            </m:cells>
                        </m:ColumnListItem>
                    </m:items>
                </m:Table>
            </m:IconTabFilter>
        </m:items>
    </m:IconTabBar>
</core:FragmentDefinition>